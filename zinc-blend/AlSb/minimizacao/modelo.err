        CPATH="${TBBROOT}/include:$CPATH"; export CPATH
    fi
else
    echo "ERROR: $LIBTBB_NAME library does not exist in $TBBROOT/lib/$library_directory."
    return 2
fi

if [ -z "${CPATH}" ]; then
    CPATH="${PSTLROOT}/include"; export CPATH
else
    CPATH="${PSTLROOT}/include:$CPATH"; export CPATH
fi
